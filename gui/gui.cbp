<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="PMF GUI" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Option virtualFolders="wxsmith\;dialogs\;panels\;" />
		<Build>
			<Target title="Release">
				<Option output="bin\Release\PMFApplication" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Release\" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-g" />
					<Add directory="$(#wx)\lib\gcc_dll\mswu" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="$(#wx)\lib\gcc_dll\mswu" />
				</ResourceCompiler>
				<Linker>
					<Add library="libwxmsw28u.a" />
					<Add directory="$(#wx)\lib\gcc_dll" />
				</Linker>
			</Target>
			<Target title="Debug">
				<Option output="bin\gui" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-g" />
					<Add directory="$(#wx)\lib\gcc_dll\mswu" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="$(#wx)\lib\gcc_dll\mswu" />
				</ResourceCompiler>
				<Linker>
					<Add library="libwxmsw28u.a" />
					<Add directory="$(#wx)\lib\gcc_dll" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-D__GNUWIN32__" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option='[[if (PLATFORM == PLATFORM_MSW &amp;&amp; (GetCompilerFactory().GetCompilerVersionString(_T(&quot;gcc&quot;)) &gt;= _T(&quot;4.0.0&quot;))) print(_T(&quot;-Wno-attributes&quot;));]]' />
			<Add option="-Wall" />
			<Add directory="$(#wx)\include" />
			<Add directory="$(#wx)\contrib\include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#wx)\include" />
		</ResourceCompiler>
		<Unit filename="GenerateDialog.cpp">
			<Option virtualFolder="dialogs\" />
		</Unit>
		<Unit filename="GenerateDialog.h">
			<Option virtualFolder="dialogs\" />
		</Unit>
		<Unit filename="ImagePanel.cpp">
			<Option virtualFolder="panels\" />
		</Unit>
		<Unit filename="ImagePanel.h">
			<Option virtualFolder="panels\" />
		</Unit>
		<Unit filename="PMFApplication.cpp" />
		<Unit filename="PMFApplication.h" />
		<Unit filename="PMFPanel.cpp">
			<Option virtualFolder="panels\" />
		</Unit>
		<Unit filename="PMFPanel.h">
			<Option virtualFolder="panels\" />
		</Unit>
		<Unit filename="mainFrame.cpp" />
		<Unit filename="mainFrame.h" />
		<Unit filename="resource.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="wxsmith\GenerateDialog.wxs" />
		<Unit filename="wxsmith\ImagePanel.wxs" />
		<Unit filename="wxsmith\PMFPanel.wxs" />
		<Unit filename="wxsmith\mainFrame.wxs" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<wxsmith version="1">
				<gui name="wxWidgets" src="PMFApplication.cpp" main="mainFrame" init_handlers="necessary" language="CPP">
					<load_resource file="mainFrame.xrc" />
					<load_resource file="GenerateDialog.xrc" />
					<load_resource file="PMFPanel.xrc" />
				</gui>
				<resources>
					<wxFrame wxs="wxsmith/mainFrame.wxs" src="mainFrame.cpp" hdr="mainFrame.h" xrc="mainFrame.xrc" name="mainFrame" language="CPP" />
					<wxDialog wxs="wxsmith/GenerateDialog.wxs" src="GenerateDialog.cpp" hdr="GenerateDialog.h" xrc="GenerateDialog.xrc" name="GenerateDialog" language="CPP" />
					<wxPanel wxs="wxsmith/ImagePanel.wxs" src="ImagePanel.cpp" hdr="ImagePanel.h" name="ImagePanel" language="CPP" />
					<wxPanel wxs="wxsmith/PMFPanel.wxs" src="PMFPanel.cpp" hdr="PMFPanel.h" xrc="PMFPanel.xrc" name="PMFPanel" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
